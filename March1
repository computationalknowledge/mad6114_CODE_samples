var dog = "Peanut";
let cat = "Fifi";
var six = 6;
function a(stepNumber){
    var sum = 0;
    var counter = 1;
    six += 1;
    console.log("let's try incrementing six " + six++);
    for (stepNumber  ; counter <=100  ; ){
        sum += counter += stepNumber;
    }
    return sum;
}
console.log("value of cat is outside function is " + cat);
console.log(a(5));


function b(a){
if (a > 0) {
    return "positive";
} else if (a === 0) {
    return "Itâ€™s a zero";
} else {
    return "NOT positive";
} }

var theanswer = b(-1);
console.log(theanswer);


Here we see that classes (really prototypes) in JavaScript are created using Associative Array NOTATION:

let obj = {
		studentname: "John",
		age: 17,
        studentid: "C993",
        outputStudentRecord() {
                return this.studentname + " " + this.age + " " + this.studentid;
            }
};
// console.log(obj.studentname );
console.log(obj['age']);
